<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vlist.hola">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.vlist.hola.MainActivity" />
        <activity android:name="com.vlist.hola.ChooseLoginRegistrationActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.vlist.hola.RegistrationActivity" />
        <activity android:name="com.vlist.hola.LoginActivity" />
        <activity android:name="com.vlist.hola.SettingsActivity" />
        <activity android:name="com.vlist.hola.Matches.MatchesActivity" />
        <activity android:name="com.vlist.hola.Chat.ChatActivity" />
    </application>

</manifest>